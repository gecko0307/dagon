struct SDL_Rect {
    int x, y;
    int w, h;
}

@nogc nothrow {
    bool SDL_PointInRect(const SDL_Point *p, const SDL_Rect *r) {
        return (( p.x >= r.x ) && ( p.x < ( r.x + r.w )) &&
                ( p.y >= r.y ) && ( p.y < ( r.y + r.h )));
    }
    bool SDL_RectEmpty( const( SDL_Rect )* X ) { return !X || ( X.w <= 0 ) || ( X.h <= 0 ); }
    bool SDL_RectEquals( const( SDL_Rect )* A, const( SDL_Rect )* B ) {
        return A && B &&
            ( A.x == B.x ) && ( A.y == B.y ) &&
            ( A.w == B.w ) && ( A.h == B.h );
    }
}

